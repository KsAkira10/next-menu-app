{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Program",
      "skipFiles": ["<node_internals>/**"],
      "port": 9230
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "port": 9230,
      "runtimeExecutable": "node",
      "preLaunchTask": "yarn: dev:inspect"
    }
  ]
}
